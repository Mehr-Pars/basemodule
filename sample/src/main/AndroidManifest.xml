<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mehrpars.mobile.sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:exported="false" android:name=".activity.dataBaseSample.list.MovieListWithLike"></activity>
        <activity android:exported="false" android:name=".activity.movieDetail.MovieDetailActivity" />
        <activity android:exported="false" android:name=".activity.movies.MovieListActivity" />
        <activity android:exported="false" android:name=".activity.target.TargetActivity" />
        <activity android:exported="true" android:name=".activity.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>